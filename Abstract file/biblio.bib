	Keywords = {embedded systems;formal specification;program testing;programming language semantics;software fault tolerance;embedded real-time systems;embedded systems;fault detection;formal specification;interleaving enforcing mutations;model-based development;orthogonal components;specification evaluation;statechart canonical maximum parallelism execution;statecharts model testing;statecharts semantics;statecharts-based models;test generation;tests adequacy;Acoustic testing;Embedded system;Fault detection;Formal specifications;Genetic mutations;Interleaved codes;Object oriented modeling;Petri nets;Real time systems;System testing},
	Month = sep,
