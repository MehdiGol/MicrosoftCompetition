	Doi = {10.1109/ICSME.2018.00050},
	Issn = {2576-3148},
	Keywords = {public domain software;software maintenance;software packages;package managers;technical lag;package dependency constraints;software packages;package dependency network;semantic versioning policy;Semantics;Computer bugs;Software packages;Tools;Terminology;Runtime;package distribution, dependency network, technical lag, semantic versioning, software evolution, empirical software engineering},
	Month = {Sep.},
	Pages = {404-414},
	Title = {On the Evolution of Technical Lag in the npm Package Dependency Network},
	Year = {2018},
	Bdsk-Url-1 = {https://doi.org/10.1109/ICSME.2018.00050}}

