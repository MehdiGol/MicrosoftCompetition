	Doi = {10.1109/ESEM.2009.5316017},
	Issn = {1949-3770},
	Keywords = {Boolean functions;object-oriented programming;quality assurance;risk analysis;software architecture;software metrics;software quality;software component strong-dependency;component-based system;context requirements;free-and-open source software;FOSS;software package;Boolean formula;semantic modeling;syntactic intercomponent relationship;component sensitivity;quality assurance;risk evaluation;empirical study;component-based software architecture;software quality measures;Packaging;Software packages;Open source software;Quality assurance;Software engineering;Software measurement;Application software;Encoding;Internet;Postal services},
	Month = {Oct},
