	Issn = {0950-5849},
	Journal = {Information and Software Technology},
	Keywords = {Software ecosystems, Reference coupling, Software dependencies, Technical dependencies, Inter-project dependencies, Cross references, Socio-technical},
	Pages = {174 - 189},
	Title = {Reference Coupling: An exploration of inter-project technical dependencies and their characteristics within large software ecosystems},
	Url = {http://www.sciencedirect.com/science/article/pii/S0950584919300527},
	Volume = {110},
	Year = {2019},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0950584919300527},
